<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%X{akkaTimestamp}  %X{akkaSource} %msg%n</pattern>
		</encoder>
	</appender>
	
	<logger name="com.krzywicki.hybrid.HybridIsland" level="info">
		<appender-ref ref="STDOUT" />
	</logger>


	
</configuration>